/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
   
     border-bottom:1px solid #d9d9d9;
}
.userinfo::before  {
  /* position: absolute; */
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,.4);
  z-index: 10;
}
.userinfo::after{
    content: '';
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    /* background:#DDCDA4; */
    background-image:url(data:image/jpeg;base64,); 
    background-repeat:no-repeat;
    background-size:100% 100%;
    -moz-background-size:100% 100%;
    opacity: 0.5;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  border:1px solid #4cae4c;
}

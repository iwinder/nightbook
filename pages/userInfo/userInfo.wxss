.book-grids{box-sizing:border-box;height: 100%}  

.book-grid{position:relative;float:left;padding:20px 10px;width:100%;box-sizing:border-box;border-right:1rpx solid #d9d9d9;border-bottom:1rpx solid #d9d9d9}

.book-grid_active{
  background-color: rgba(181, 200, 202,0.3)
}

.book-grid__icon{display:block;width:80rpx;height:80rpx;margin: 0 auto;}

.book-grid__label{margin-top:10px;display:block;text-align:center;color:#000;font-size:14px;white-space:nowrap;text-overflow:ellipsis;overflow:hidden}

